{
  "extends": [
    "eslint:recommended", "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": 1,
    "react/display-name": 0,
    "react/no-danger": 1,
    "react/prop-types": 1,
    "no-unused-vars": ["warn", { "ignoreRestSiblings": true }],
    "semi": [1, "never"]
  }
}
